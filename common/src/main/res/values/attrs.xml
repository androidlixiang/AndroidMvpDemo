<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTitleView">
        <!--标题名字-->
        <attr name="titleName" format="string" />
        <!--右边的资源文件-->
        <attr name="rightRes" format="reference" />
        <!--左边的资源文件-->
        <attr name="leftRes" format="reference" />
        <!--标题颜色-->
        <attr name="titleColor" format="color" />
        <!--设置自己的布局文件-->
        <attr name="uselayout" format="integer" />
        <!--系统状态栏-->
        <attr name="showMstate" format="boolean" />
        <!--是否显示下划线-->
        <attr name="showLine" format="boolean" />
        <!--  左边的返回按钮是否显示-->
        <attr name="showBack" format="boolean" />
    </declare-styleable>

</resources>